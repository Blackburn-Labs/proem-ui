{
  "name": "proem-ui",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "lint": "cross-env eslint . --ext .js --ext .jsx  --ignore-path .gitignore --cache",
    "test": "karma start --single-run karma.conf.js",
    "test:watch": "karma start --single-run=false --watch --browsers Chrome karma.conf.js",
    "bundle:prod": "cross-env webpack --progress --config webpack.prod.js",
    "bundle:stage": "cross-env webpack --progress --config webpack.stage.js",
    "bundle:dev": "cross-env webpack --progress --config webpack.dev.js",
    "bundle:local": "cross-env webpack --progress --config webpack.local.js",
    "bundle:watch": "cross-env webpack --progress --config webpack.local.js --watch",
    "rev-parse": "git rev-parse HEAD > gitBuildHash.json",
    "prebundle": "npm run rev-parse",
    "bundle": "npm run bundle:prod",
    "clean": "rimraf dist/*",
    "start:client": "electron .",
    "start": "webpack-dev-server --inline --hot --config webpack.local.js",
    "package": "electron-packager . --out dist/ --ignore \"(.idea|actions|pages|reducers|utils|test|dist|node_modules/electron.*)\" --icon icons/favicon.ico --overwrite",
    "build": "npm run clean && npm run bundle && npm run package",
    "deploy": "node deploy root=app",
    "deploy:dev": "node deploy root=edge",
    "deploy:stage": "node deploy root=stage"
  },
  "repository": "https://github.com/rwblackburn/proem-ui.git",
  "author": {
    "name": "RWBlackburn, LLC (b.d.a. Blackburn Labs)",
    "email": "rob@blackburnlabs.com",
    "url": "http://www.blackburnlabs.com"
  },
  "license": "none",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@material-ui/core": "^4.1.3",
    "@material-ui/lab": "^4.0.0-alpha.20",
    "axios": "^0.19.0",
    "classnames": "^2.2.5",
    "copyfile": "^0.1.2",
    "cross-env": "^5.2.0",
    "electron": "^5.0.6",
    "electron-installer-windows": "^2.0.0",
    "electron-packager": "^14.0.1",
    "electron-prebuilt": "*",
    "fabric": "^3.2.0",
    "face-api.js": "^0.20.1",
    "fs": "0.0.1-security",
    "moment": "^2.22.2",
    "notistack": "^0.8.8",
    "preloader": "^4.0.3",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.6.1",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-ga": "^2.6.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-transition-group": "^4.2.1",
    "react-vis": "^1.11.6",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.1",
    "redux-thunk": "^2.1.0",
    "scheduler": "^0.13.6",
    "showdown": "^1.9.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/plugin-transform-object-set-prototype-of-to-assign": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "braces": "^2.3.2",
    "clone": "^2.0.0",
    "css-loader": "^3.0.0",
    "deep-equal": "^1.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "fs-plus": "*",
    "ftp-deploy": "^2.1.1",
    "history": "^4.7.2",
    "immutability-helper": "^3.0.1",
    "jasmine": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "*",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "puppeteer": "^1.18.1",
    "renamer": "^1.1.2",
    "replace-in-file": "^4.1.2",
    "rimraf": "*",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "url-loader": "^2.0.1",
    "yargs": "^13.2.4"
  }
}
