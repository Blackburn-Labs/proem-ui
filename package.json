{
  "name": "proem-ui",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "lint": "cross-env eslint . --ext .js --ext .jsx  --ignore-path .gitignore --cache",
    "test": "karma start --single-run karma.conf.js",
    "test:watch": "karma start --single-run=false --watch --browsers Chrome karma.conf.js",
    "bundle:prod": "cross-env webpack --progress --config webpack.prod.js",
    "bundle:stage": "cross-env webpack --progress --config webpack.stage.js",
    "bundle:dev": "cross-env webpack --progress --config webpack.dev.js",
    "bundle:local": "cross-env webpack --progress --config webpack.local.js",
    "bundle:watch": "cross-env webpack --progress --config webpack.local.js --watch",
    "rev-parse": "git rev-parse HEAD > gitBuildHash.json",
    "prebundle": "npm run rev-parse",
    "bundle": "npm run bundle:prod",
    "clean": "rimraf dist/*",
    "start": "electron .",
    "start:web": "webpack-dev-server --inline --hot --config webpack.local.js",
    "package": "electron-packager . --out dist/ --ignore \"(.idea|actions|pages|reducers|utils|test|dist|node_modules/electron.*)\" --icon icons/favicon.ico --overwrite",
    "build": "npm run clean && npm run bundle && npm run package",
    "deploy": "node deploy root=app",
    "deploy:dev": "node deploy root=edge",
    "deploy:stage": "node deploy root=stage"
  },
  "repository": "git@bitbucket.org:blackburnlabs/dynamic-calculator-ui.git",
  "author": {
    "name": "RWBlackburn, LLC (b.d.a. Blackburn Labs)",
    "email": "rob@blackburnlabs.com",
    "url": "http://www.blackburnlabs.com"
  },
  "license": "none",
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "axios": "^0.15.3",
    "classnames": "^2.2.5",
    "electron": "^1.4.13",
    "electron-installer-windows": "*",
    "electron-packager": "*",
    "electron-prebuilt": "*",
    "moment": "^2.22.2",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.6.1",
    "react-dom": "16.6.1",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-tap-event-plugin": "^3.0.3",
    "react-transition-group": "^2.5.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-promise-middleware": "^4.1.0",
    "redux-thunk": "^2.1.0",
    "react-vis": "^1.11.6",
    "scheduler": "^0.11.1",
    "showdown": "^1.9.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-async-functions": "^6.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.1.4",
    "babel-polyfill": "^6.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clone": "^2.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.26.1",
    "deep-equal": "^1.0.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "file-loader": "^0.10.1",
    "fs-plus": "*",
    "ftp-deploy": "^2.1.1",
    "history": "^4.7.2",
    "immutability-helper": "^2.1.2",
    "jasmine": "^2.5.3",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "*",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "puppeteer": "^1.2.0",
    "rimraf": "*",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "yargs": "*"
  }
}
